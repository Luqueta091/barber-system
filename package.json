  "main": "dist/main/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/main/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node -r tsconfig-paths/register dist/main/server.js",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest --runInBand --passWithNoTests",
    "prisma:migrate:init": "sh -c 'export $(grep -v '^#' .env | xargs); DATABASE_URL=\"$DIRECT_DATABASE_URL\" prisma migrate dev --name init'",
    "prisma:migrate": "sh -c 'export $(grep -v '^#' .env | xargs); DATABASE_URL=\"$DIRECT_DATABASE_URL\" prisma migrate dev'",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
